<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.labb3wassimtajeddin.PaintViewController">
    <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
        <top>
            <HBox spacing="10.0" BorderPane.alignment="CENTER">
                <Button fx:id="circleButton" lineSpacing="1.0" mnemonicParsing="false" onAction="#onCircleAction" text="Circle">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets></Button>
                <Button fx:id="rectangleButton" lineSpacing="1.0" mnemonicParsing="false" onAction="#onRectangleAction" text="Rectangle" />
            </HBox>
        </top>
        <right>
            <VBox BorderPane.alignment="CENTER">
                <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onSaveAction" text="Save" />
                <Button fx:id="undoButton" mnemonicParsing="false" onAction="#onUndoAction" prefHeight="25.0" prefWidth="52.0" text="Undo" />
            </VBox>
        </right>
        <center>
            <Canvas fx:id="canvas" height="373.0" onMouseClicked="#onCanvasClicked" width="525.0" BorderPane.alignment="CENTER" />
        </center>
    </BorderPane>

</AnchorPane>
